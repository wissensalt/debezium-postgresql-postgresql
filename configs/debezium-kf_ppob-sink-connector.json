{
  "name": "postgres-kf_ppob-sink-connector",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "1",

    "connection.url": "jdbc:postgresql://postgres_target:5432/kf_ppob?user=postgres&password=pgadmin",
    "connection.user": "postgres",
    "connection.password": "pgadmin",

    "topics": "postgre-kf_ppob-source.public.phone_credits",
    "dialect.name": "PostgreSqlDatabaseDialect",

    
    "insert.mode": "upsert",
    "pk.fields": "id",
    "pk.mode": "record_key",
    "delete.enabled": "true",
    "auto.evolve": true,

    "transforms": "unwrap",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "transforms.unwrap.drop.tombstones": "false",
    "transforms.unwrap.delete.handling.mode": "rewrite",
    "transforms.unwrap.add.fields": "table,lsn"
  }
}